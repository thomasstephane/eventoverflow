<h1>All Events</h1>

<% @events.reverse.each do |e| %>
  <p>
    <li><%= link_to e.title, e %></li>
    <li><%= link_to 'Edit', edit_event_path(e) %></li>
    <li><%= link_to 'Delete', e, :method => :delete %></li>
    <li><%= render :partial => 'votes/vote_event' , :locals => {:event_id => e.id }%></li>
  </p>
<% end %>
